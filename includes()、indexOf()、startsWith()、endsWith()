<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
</head>
<body>
  <script>

    /*includes()和indexof(),既是string的方法，又是array的方法*/

  var array = [1,2,3,4,5,6];
  var str= 'abcd'
  if(array.includes(4) ){
    console.log("在数组中找到4");
  } 

  if(str.includes('b')) {
    console.log('字符串中包含b')
  }

  //使用includes（）方法检查NaN,使用indexOf（）检查不出来

	
  var  array = [NaN];
    
  if (array.includes(NaN)){
     console.log("1在数组中找到NaN");
  }

  if (array.indexOf(NaN) != -1){
     console.log("2在数组中找到NaN");
  } else {
    console.log("3在数组中没有找到NaN");
  }

  //使用includes() 方法检查undefined 。使用indexof()检查不出来
  var array = [, , , ,];
 
  if(array.includes(undefined)){
    console.log("1在数组中找到undefined");
  } 

  if(array.indexOf(undefined) != -1) {
    console.log("2在数组中找到undefined");
  } else {
    console.log("3在数组中没有找到undefined");
  }

  //startsWith和endsWith是string的方法，不是array的方法

  var str = 'abcd '
  
  if(str.startsWith('a')){
    console.log('a是在字符串的第一位')
  }

  if (str.endsWith('')) {
    console.log('空格是在字符串的最后一位')
  }


  

  </script>
</body>
</html> 
